# Example environment variables for apps that use this database.
# Copy this file to `.env` and fill in real values.

DB_ENGINE=sqlite       # sqlite, postgres, or mysql
DB_NAME=business.db    # database name or file
DB_USER=app_user       # PostgreSQL / MySQL username
DB_PASSWORD=secret     # PostgreSQL / MySQL password
DB_HOST=localhost
DB_PORT=5432           # 5432 for Postgres, 3306 for MySQL
